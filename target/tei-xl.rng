   <define name="name">
      <element name="name">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(name,
            proper noun) contains a proper noun or noun phrase.</a:documentation>
         <ref name="macro.phraseSeq"/>
